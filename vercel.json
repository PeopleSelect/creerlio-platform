{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd frontend && npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "frontend/app/**/*.ts": {
      "runtime": "nodejs24.x"
    },
    "frontend/app/**/*.tsx": {
      "runtime": "nodejs24.x"
    }
  }
}
